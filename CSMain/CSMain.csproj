<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Include="Plugins\**\*" CopyToOutputDirectory="PreserveNewest" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CSMainAPI" Version="1.0.4" />
  </ItemGroup>

  <ItemGroup>
    <Using Include="CSMainAPI" Global="true" />
    <Using Include="CSMainAPI.CSMainAPI" Global="true" Static="true" />
  </ItemGroup>

</Project>
